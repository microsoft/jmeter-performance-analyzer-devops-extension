{
    "manifestVersion": 1,
    "id": "jmeter-perf-analyzer",
    "name": "JMeter Performance Analyzer",
    "version": "1.3.81",
    "publisher": "id-az-pipeline",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "This task enables to run Performance testing using Apache Jmeter, Analyze report and post results.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [

        { "path": "perfanalyzer" },
        {
            "path": "overview.md",
            "addressable": true
        },
        {
            "path": "images/extension-icon.png",
            "addressable": true
        },
        {
            "path": "screenshots",
            "addressable": true
          }
    ],
    "screenshots": [
        { "path": "screenshots/P4.png"},
        { "path": "screenshots/P5.png"},
        { "path": "screenshots/P10.png"}
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "sample": {
            "path": "samples/README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "repository": {
    "type": "git",
    "uri": "https://github.com/microsoft/jmeter-performance-analyzer-devops-extension"
    },
    "links": {

        "repository": {
            "uri": "https://github.com/microsoft/jmeter-performance-analyzer-devops-extension"
        },
        "issues": {
            "uri": "https://github.com/microsoft/jmeter-performance-analyzer-devops-extension/issues"
        },
        "samples": {
            "uri" : "https://github.com/microsoft/jmeter-performance-analyzer-devops-extension/blob/main/samples/README.md"
        },
        "readMe": {
            "uri" : "https://github.com/microsoft/jmeter-performance-analyzer-devops-extension/blob/main/README.md"
        }
    },

    "branding": {
        "color": "#FFFFFF",
        "theme": "light"
    },
    "tags": [
        "performance",
        "performance testing",
        "perf analyzer",
        "performanceanalyzer",
        "coverage",
        "publish",
        "report",
        "publishreport",
        "SSDT",
        "Apache",
        "ApacheJMeter",
        "ApacheJMeterPerformanceTest",
        "coverage",
        "tag"
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "perfanalyzer"
            }
        }
    ]

}
